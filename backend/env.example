PORT=3000
NODE_ENV=development
DB_PATH=./data/made.db
UPLOAD_DIR=./uploads
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
MODEL_PATH=./models/malaria_model.json

# Configuration de l'analyse ML
# Options: 'sharp' (par défaut - analyse d'image réelle), 'google', 'aws', 'tensorflow'
ML_ANALYSIS_PROVIDER=sharp

# Google Vision API (optionnel - si vous utilisez Google Vision)
# Obtenir la clé sur: https://cloud.google.com/vision/docs/setup
GOOGLE_VISION_API_KEY=your-google-vision-api-key-here

# AWS Rekognition (optionnel - si vous utilisez AWS)
# Nécessite aussi d'installer: npm install aws-sdk
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here

# APIs externes - Notifications (SMS, WhatsApp, Push)
# SMS Provider (Twilio, AWS SNS, etc.)
SMS_API_KEY=your-sms-api-key-here
SMS_API_URL=https://api.sms-provider.com/send

# WhatsApp Business API (Meta, Twilio, etc.)
WHATSAPP_API_KEY=your-whatsapp-api-key-here
WHATSAPP_API_URL=https://api.whatsapp.com/v1/messages

# Firebase Cloud Messaging (Push notifications)
FCM_SERVER_KEY=your-fcm-server-key-here

# USSD Gateway (si disponible)
USSD_API_KEY=your-ussd-api-key-here
USSD_API_URL=https://api.ussd-gateway.com/send

# Gemini AI Chatbot
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=models/gemini-2.5-flash

# Storage (S3/MinIO)
STORAGE_PROVIDER=local
S3_BUCKET=made-diagnostics
# Pour AWS S3
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here
# Pour MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=your-minio-access-key-here
MINIO_SECRET_KEY=your-minio-secret-key-here
MINIO_REGION=us-east-1

# Redis (pour BullMQ)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

